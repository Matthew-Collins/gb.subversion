' Gambas class file

Public RunObserver As Observer

Public Sub _new()

  RunObserver = New Observer(Global.Run)

End

Public Sub RunObserver_Executing(TextCommands As String[])

  TextCommands.Text &= "Command: " & TextCommands.Join(" ")

End

Public Sub RunObserver_ErrorMessage(Value As String)

  TextCommands.Text &= "Error: " & Value

End

Public Sub Form_Open()

  Test_001_DisplayReadAndErrors.Main

End
